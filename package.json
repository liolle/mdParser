{
  "name": "@edllx/md-parser",
  "version": "2.0.0",
  "description": "Markdown parser",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "example": "example"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "clean": true
  },
  "scripts": {
    "test": "vitest --config ./vitest.config.ts",
    "test:once": "vitest run --config ./vitest.config.ts",
    "start": "tsx src/main.ts",
    "lint": "tsc",
    "release": "pnpm build && pnpm changeset publish",
    "build": "tsup"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.5.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
