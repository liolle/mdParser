{
  "name": "md-parser",
  "version": "1.0.0",
  "description": "Markdown parser",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "example": "example"
  },
  "tsup": {
    "entry": [
      "index.ts",
      "compiler/compiler.ts",
      "compiler/md.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "test": "vitest --config ./vitest.config.ts",
    "start": "tsx main.ts",
    "lint": "tsc",
    "build": "tsup --format cjs,esm --dts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.5.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
